<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    
    <bean class="com.DependencyInjection.Address" name="a1" >
    <property name="cname" value="Mengadewadi"></property>
    <property name="laneNo" value="3"></property>
    </bean>
    
    
    <bean class="com.DependencyInjection.Student" name="s1" >
    <property name="name" value="Vivek" ></property>
    <property name="id" value="101" ></property>
    <property name="add" ref="a1" ></property>
    
    </bean>
    
    
</beans>